```{r}
library(tidyverse);library(exscidata)

data("hypertrophy")

glimpse(hypertrophy)
  
hypertrophy %>%
  select(PARTICIPANT, TESTOSTERONE_T1:TESTOSTERONE_T3, 
         DXA_LBM_T1:DXA_LBM_T3) %>%
 
  pivot_longer(names_to = "variable",
              values_to = "values",
              cols = TESTOSTERONE_T1:DXA_LBM_T3) %>%
  

  mutate(variable = gsub("_LBM", "", variable)) %>%
  
  separate(variable, into = c ("variable", "time"), sep ="_") %>%
  
print()




```

